{
    "name": "chubes/sell-my-images",
    "description": "WordPress plugin for monetizing images with AI upscaling",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Chris Huber",
            "homepage": "https://chubes.net"
        }
    ],
    "require": {
        "php": ">=7.4",
        "stripe/stripe-php": "^10.0"
    },
    "require-dev": {
        "brain/monkey": "^2.6",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^9.6",
        "yoast/phpunit-polyfills": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SellMyImages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SellMyImages\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:unit": "phpunit --testsuite unit"
    },
    "config": {
        "sort-packages": true
    }
}